include ../injected/injected
include ../head/head
include ../scripts/scripts
include ../../helpers/variables


mixin page(data) 
    body.page&attributes(attributes)
      
      +injected: block

      .page__wrapper
        +if('header')
          +use('header')

        +if('main')
          section.page__main
            +use('main')

        +if('footer')
          +use('footer')
  
      +scripts()
      //- +popup()
      //- +notification()